# Unix
alias ll="ls -al"
alias mkdir="mkdir -p"
alias tmux0="tmux attach || tmux"

# Pretty print the path
alias path='echo $PATH | tr -s ":" "\n"'

alias code-server="~/openvscode-server/server.sh --port=3000"

# Include custom aliases
[[ -f ~/.aliases.local ]] && source ~/.aliases.local

